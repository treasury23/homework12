{% extends "::base.html.twig" %}

{% block body %}
{% for item in items %}
    <div class="wrapper">
        <div class="title">{{ item.title }}</div>
        <div class="img_float"><img src="{{ item.image.path | imagine_filter('my_thumb')  }}"></div>
        <div>{{ item.body }}</div>
    </div>
    <div class="sidebar">
        <div class="article">
            <div class="title">Last Articles</div>
        </div>
        <div class="post-guestbook">
            <div class="title">Last post from questbook</div>
        </div>
    </div>
{% endfor %}

{% endblock body %}
